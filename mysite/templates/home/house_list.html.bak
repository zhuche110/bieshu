      <script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>
      <!-- 包括所有已编译的插件 -->
      <title>别墅图纸大全-追求美好生活</title>
   </head>
<body>
<div class="container">
   <div class="row">
      <div class="col-lg-2">
	<table class='table'>
	{% for house in house_list %}
        <tr><td>{{house.title}}</td>
        </tr>
	{% endfor %}
	</table>	
	</div>
	<div class="col-lg-8">
	<table class='table'>
	{% for house in house_list %}
	<tr><td>{{house.content |safe}}</td></tr>
	{% endfor %}
	 </table>
	</div>
    </div>
</div>




</body>
</html>
